 = _(this.context)\n      .groupBy(month)\n      .mapValues(items => _.map(items, 'priority'))\n      .value()\n\n    const highCount = (month) => {\n      const high = [];\n      month.map(item => {\n        if (item === 'High') { high.push(item) };\n        return item;\n      });\n      return high.length;\n    };\n\n    const mediumCount = (month) => {\n      const medium = [];\n      month.map(item => {\n        if (item === 'Medium') { medium.push(item) };\n        return item;\n      });\n      return medium.length;\n    };\n\n    const lowCount = (month) => {\n      const low = [];\n      month.map(item => {\n        if (item === 'Low') { low.push(item) };\n        return item;\n      });\n      return low.length;\n    };\n\n    const series = [\n      {\n        name: \"All tasks\",\n        data: [\n          groups.Jan.length,\n          groups.Feb.length,\n          groups.Mar.length,\n          groups.Apr.length,\n          groups.May.length,\n          groups.Jun.length\n        ]\n      },\n      {\n        name: \"Low priority\",\n        data: [\n          lowCount(groups.Jan),\n          lowCount(groups.Feb),\n          lowCount(groups.Mar),\n          lowCount(groups.Apr),\n          lowCount(groups.May),\n          lowCount(groups.Jun)\n        ]\n      },\n      {\n        name: \"Medium priority\",\n        data: [\n          mediumCount(groups.Jan),\n          mediumCount(groups.Feb),\n          mediumCount(groups.Mar),\n          mediumCount(groups.Apr),\n          mediumCount(groups.May),\n          mediumCount(groups.Jun)\n        ]\n      },\n      {\n        name: \"High priority\",\n        data: [\n          highCount(groups.Jan),\n          highCount(groups.Feb),\n          highCount(groups.Mar),\n          highCount(groups.Apr),\n          highCount(groups.May),\n          highCount(groups.Jun)\n        ]\n      }\n    ];\n    \n    return (\n      <div className=\"wrapper-timeline\">\n        <div className=\"title-wrapper\">\n          <h2><ScheduleIcon /> Timeline</h2>\n        </div>\n        <div id=\"chart\">\n          <ReactApexChart options={this.state.options} series={series} type=\"line\" height={350} />\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Timeline;","/Users/vascooliveira/code/volivers/react-moma-dashboard/src/index.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/components/tasks/TaskForm.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/components/tasks/Task.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/styles/TaskStyles.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/styles/TaskListStyles.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/styles/TaskFiltersStyles.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/components/tasks/TaskFilters.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/styles/TaskFormStyles.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/components/common/Footer.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/contexts/tasks.context.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/reducers/tasks.reducer.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/constants/actions.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/data/makeData.jsx",[],["129","130"],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/hooks/useInputState.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/components/Dashboard.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/contexts/artworks.context.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/contexts/users.context.jsx",[],"/Users/vascooliveira/code/volivers/react-moma-dashboard/src/components/metrics/CompletedTasks.jsx",["131"],"import React, { Component } from 'react';\nimport { TasksContext } from '../../contexts/tasks.context';\nimport ReactApexChart from 'react-apexcharts';\n\nclass CompletedTasks extends Component {\n  static contextType = TasksContext;\n\n  constructor(props) {\n    super(props);\n    this.state = {\n      options: {\n        chart: {\n          height: 200,\n          width: 200,\n          type: 'radialBar',\n          offsetY: -10,\n          sparkline: {\n            enabled: true\n          }\n        },\n        grid: {\n          padding: {\n              right: 0,\n              left: 0,\n          }\n      \t},\n        plotOptions: {\n          radialBar: {\n            startAngle: -135,\n            endAngle: 135,\n            dataLabels: {\n              name: {\n                fontSize: '14px',\n                color: 'rgba(0, 0, 0, 0.87)',\n                offsetY: 80\n              },\n              value: {\n                offsetY: 40,\n                fontSize: '18px',\n                color: 'rgba(0, 0, 0, 0.87)',\n                formatter: function (val) {\n                  return val + \"%\";\n                }\n              }\n            }\n          }\n        },\n        fill: {\n          type: 'gradient',\n          gradient: {\n              shade: 'dark',\n              shadeIntensity: 0.15,\n              inverseColors: false,\n              opacityFrom: 1,\n              opacityTo: 1,\n              stops: [0, 50, 65, 91]\n          },\n        },\n        stroke: {\n          dashArray: 2\n        },\n        labels: ['Completed Tasks'],\n      },\n    };\n  }\n\n  render() {\n    const { tasks } = this.context;\n    const completed = [];\n    this.context.map(task => task.completed === true ? completed.push(task) : task);\n\n    let ratio;\n    if (completed.length !== 0) {\n      ratio = [Math.floor((completed.length / this.context.length) * 100)];\n    } else {\n      ratio = [0];\n    };\n\n    return (\n      <div id=\"chart\">\n        <ReactApexChart options={this.state.options} series={ratio} type=\"radi