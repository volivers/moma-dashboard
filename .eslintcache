diumCount = (month) => {\n      const medium = [];\n      month.map(item => {\n        if (item === 'Medium') { medium.push(item) };\n        return item;\n      });\n      return medium.length;\n    };\n\n    const lowCount = (month) => {\n      const low = [];\n      month.map(item => {\n        if (item === 'Low') { low.push(item) };\n        return item;\n      });\n      return low.length;\n    };\n\n    const series = [\n      {\n        name: \"All tasks\",\n        data: [\n          groups.Jan.length,\n          groups.Feb.length,\n          groups.Mar.length,\n          groups.Apr.length,\n          groups.May.length,\n          groups.Jun.length\n        ]\n      },\n      {\n        name: \"Low priority\",\n        data: [\n          lowCount(groups.Jan),\n          lowCount(groups.Feb),\n          lowCount(groups.Mar),\n          lowCount(groups.Apr),\n          lowCount(groups.May),\n          lowCount(groups.Jun)\n        ]\n      },\n      {\n        name: \"Medium priority\",\n        data: [\n          mediumCount(groups.Jan),\n          mediumCount(groups.Feb),\n          mediumCount(groups.Mar),\n          mediumCount(groups.Apr),\n          mediumCount(groups.May),\n          mediumCount(groups.Jun)\n        ]\n      },\n      {\n        name: \"High priority\",\n        data: [\n          highCount(groups.Jan),\n          highCount(groups.Feb),\n          highCount(groups.Mar),\n          highCount(groups.Apr),\n          highCount(groups.May),\n          highCount(groups.Jun)\n        ]\n      }\n    ];\n    \n    return (\n      <div className=\"wrapper-timeline\">\n        <div className=\"title-wrapper\">\n          <h2><ScheduleIcon /> Timeline</h2>\n        </div>\n        <div id=\"chart\">\n          <ReactApexChart options={this.state.options} series={series} type=\"line\" height={350} />\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Timeline;","/Users/vascooliveira/code/volivers/react-moma-dashboard/src/index.jsx",[]